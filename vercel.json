{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/api/my_endpoint.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/api/my_endpoint.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ],
    "env": {
      "PYTHONPATH": "backend"
    }
  }
  